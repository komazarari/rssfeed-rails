<p id="notice"><%= notice %></p>

<h1>Posts</h1>
<div><%= link_to 'RSS2.0', feed_posts_url %></div>
<div><%= link_to 'feed', feed_posts_url.gsub(/^https?/,'feed') %></div>
<hr>
    <% @posts.each do |post| %>
    <h3><%= link_to post.title, post_path(post) %></h3>
        <ul>
          <li /><%= post.created_at.localtime %>
          <li /><%= post.description %>
        </ul>

    <% end %>


<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
  </tbody>
</table>

<br>

<%= link_to 'New Post', new_post_path %>
